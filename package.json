  "license": "BSD-2-Clause",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/is-webcomponents-supported.git"
  },
  "scripts": {
    "build": "browserify -e ./index.js -s isWebComponentsSupported > dist/index.js",
    "eslint": "eslint --fix --cache .",
